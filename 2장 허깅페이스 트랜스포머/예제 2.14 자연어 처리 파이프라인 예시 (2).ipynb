    "from transformers import pipeline\n",
    "\n",
    "pipe = pipeline(\n",
    "    task=\"text-classification\",\n",
    "    model=\"cardiffnlp/twitter-roberta-base-sentiment-latest\"\n",
    ")\n",
    "raw_text = [\n",
    "    \"I love you.\",\n",
    "    \"I hate you.\",\n",
    "    \"I meet with you.\"\n",
    "]\n",
    "prediction = pipe(raw_text)\n",
    "print(prediction)"
   ]
  }
